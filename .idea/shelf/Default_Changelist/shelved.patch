+package com.company;
+
+public class Main {
+
+    public static void main(String[] args) {
+	// создали пару магазинов (обращается в класс Shop за параметрами)
+        Shop lenta = new Shop(100, "Lenta");
+        Shop ikea = new Shop(200, "IKEA");
+
+    // создали пару продуктов (обращается в класс Products за параметрами)
+        Products pasta1 = new Products(100,  "Pasta Zara 0,5 kg",    "food", 300);
+        Products pasta2 = new Products(500,  "Pasta Barilla 500 g", "food", 300);
+        Products table1 = new Products(2000, "tableFlat",    "furniture", 300);
+        Products table2 = new Products(3000, "tableRound",   "furniture", 300);
+        lenta.addProduct("Beacon", 100);
+
+    // добавили продукты в магазины (обращается в метод addProduct класса Shop и в класс Products за именем)
+        lenta.addProduct(pasta1.getProductName(), 100);
+        lenta.addProduct(pasta2.getProductName(),100);
+        ikea.addProduct(table1.getProductCategory(), 100);
+        ikea.addProduct(table2.getProductCategory(), 100);
+        ikea.addProduct(table1.getProductName(), 100);
+        ikea.addProduct(table2.getProductName(), 100);
+
+        String saleStatus =  lenta.sellProduct("pasta", 20);
+
+        System.out.println("Product category: " + pasta1.getProductCategory() + "; Product name: " + pasta1.getProductName() + "; Product Retail Price = " + pasta1.getPrice());
+        System.out.println("\r");
+        System.out.println("Product category: " + pasta2.getProductCategory() + "; Product name: " + pasta2.getProductName() + "; Product Retail Price = " + pasta2.getPrice());
+        int a = 1;
+    }
+}
