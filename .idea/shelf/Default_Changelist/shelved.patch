+package com.company;
+
+// обозначаем поля класса
+public class Products {
+    int price;
+    String productName;
+    String productCategory;
+    int initialQuantity;
+
+// конструктор
+    public Products(int price, String productName, String productCategory, int initialQuantity) {
+        this.price = price;
+        this.productName = productName;
+        this.productCategory = productCategory;
+        this.initialQuantity = initialQuantity;
+    }
+
+// геттеры (для передачи в другие классы)
+    public int getPrice() {
+        return price;
+    }
+    public String getProductName() {
+        return productName;
+    }
+    public String getProductCategory() {
+        return productCategory;
+    }
+    public int getInitialQuantity(){return initialQuantity;}
+}
