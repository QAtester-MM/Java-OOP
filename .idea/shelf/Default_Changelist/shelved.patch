+package com.company;
+
+import java.util.ArrayList;
+
+public class Shop {
+    private String shopName;
+    private int shopID;
+    private ArrayList<String> productName;
+    private ArrayList<Integer> stockQuantity;
+
+    public Shop(int shopID, String shopName) {
+        this.shopID = shopID;
+        this.shopName = shopName;
+        this.productName = new ArrayList<>();
+        this.stockQuantity = new ArrayList<>();
+
+    }
+
+    public void addProduct(String productName, int stockQuantity){
+        this.productName.add(productName);
+        this.stockQuantity.add(stockQuantity);
+
+    }
+
+    public String sellProduct(String productName, int quantitySold){
+//        this.productName.add(productName);
+
+        int initialQuantity = this.stockQuantity.get(1);
+        if ( quantitySold <= initialQuantity){
+            this.stockQuantity.set(1, initialQuantity - quantitySold);
+            return "Успешная продажа";
+        }
+        else return "Ошибка в количестве";
+    }
+}
